<div class="container" style="width:100%!important">
    <style>
        .e-headercell {
            background-color: #14567e !important;
            color: white;
        }

        .e-summarycell {
            background-color: #8fbfdc !important;
        }
    </style>
    <div class="row">
        <div class="col-md-12">
            <div class="bg-primary" style="padding-top:5px; padding-Center:5px;">
                <center>
                    <label style="color:white;">@ViewBag.heading</label>
                    <label style="color: white;padding-right: 10px;" class="pull-right"><strong>TOTAL RECORD : @ViewBag.Total </strong></label>
                </center>
            </div>
        </div>
    </div>
    <div class="table-responsive">
        @(Html.EJS().Grid("GrdTOTHookUpDate").DataSource((System.Data.DataTable)ViewBag.DataSource).
            Height("400").
            AllowPaging(true).
            PageSettings(Page => Page.PageSize(20)).
            AllowSelection(false).
            AllowResizing().
            AllowExcelExport().
            ToolbarClick("toolbarClick").
            AllowFiltering().
            AllowGrouping().
            AllowSorting().
            //AllowSorting().EnableColumnVirtualization().EnableVirtualization().
            Columns(col =>
            {
                //col.Field("PLAN_ID").HeaderText("PLAN ID").Add();
                col.Field("FCODE").HeaderText("FCODE").Width("120").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                //col.Field("OIL QTY").HeaderText("OIL QTY").Add();
                col.Field("DESCRIPTION").HeaderText("DESCRIPTION").Width("180").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("SHORT_CODE").HeaderText("SHORT CODE").Width("120").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("JOB").HeaderText("JOB").Width("120").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("HOOK_NO").HeaderText("HOOK NO").Width("120").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("ENTRY_DATE").HeaderText("HOOKUP DATE").Width("180").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("HYDRAULIC_LIFT").HeaderText("HYDRAULIC LIFT").Width("130").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();

                col.Field("STARTER_MOTOR_SRLNO").HeaderText("STARTER MOTOR SRLNO").Width("180").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("ALTERNATOR_SRLNO").HeaderText("ALTERNATOR SRLNO").Width("160").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("STEERING_MOTOR_SRLNO").HeaderText("STEERING MOTOR SRLNO").Width("180").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("HYD_PUMP_SRLNO").HeaderText("HYD PUMP SRLNO").Width("180").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("REMARKS").HeaderText("REMARKS").Width("160").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();

                col.Field("BUCKLE UP DATE").HeaderText("BUCKLE UP DATE").Width("160").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("TRANSMISSION").HeaderText("TRANSMISSION").Width("160").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("REARAXEL").HeaderText("REARAXEL").Width("140").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("SKID").HeaderText("SKID").Width("130").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("ENGINE").HeaderText("ENGINE").Width("130").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                col.Field("TRACTOR_NO").HeaderText("TRACTOR NO").Width("140").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                //col.Field("SIM_SERIAL_NO").HeaderText("SIM SERIAL NO").Add();

                //col.Field("IMEI_NO").HeaderText("IMEI NO").Add();
                //col.Field("MOBILE").HeaderText("MOBILE").Add();

                col.Field("FIPSRNO").HeaderText("FIPSRNO").Width("140").TextAlign(Syncfusion.EJ2.Grids.TextAlign.Center).Add();
                //col.Field("ROPS_SRNO").HeaderText("ROPS SRNO").Add();
                //col.Field("BATTERY_SRLNO").HeaderText("BATTERY SRLNO").Add();
                //col.Field("BATTERY_MAKE").HeaderText("BATTERY MAKE").Add();
                //col.Field("ROLL OUT DATE").HeaderText("ROLL OUT DATE").Add();
                //col.Field("PDI OK TIME").HeaderText("PDI OK TIME").Add();
                //col.Field("HOOK UP DATE").HeaderText("HOOK UP DATE").Add();
            }).
            Toolbar(new List<string>() { "ExcelExport" }).
            Render())
        <script>
            function toolbarClick(args) {
                var gridObj = document.getElementById("GrdTOTHookUpDate").ej2_instances[0];
                gridObj.excelExport();
            }
        </script>
    </div>
</div>
<script src="@Url.Content("~/Scripts/ej2/ej2.min.js")"></script>

@Html.EJS().ScriptManager()









