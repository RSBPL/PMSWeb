<script>

    $(document).ready(function () {
        $.noConflict();
        DDLPlant();
        BindStage();
        AutoComplete();
    });


    function AutoComplete() {
        $("#JOBID").autocomplete({
            source: function (request, response) {

                var Data = {
                    PLANTCODE: $('#PLANTCODE').val(),
                    FAMILYCODE: $('#FAMILYCODE').val(),
                    JOBID: $('#JOBID').val()
                };
                $.ajax({
                    url:  '@Url.Action("FillITEMCODE", "RollDown")',
                    type: "POST",
                    contentType: 'application/json;charset=utf-8',
                    dataType: "json",
                    data: JSON.stringify({ data: Data }),
                    success: function (data) {
                        response($.map(data, function (item) {
                            /*console.log(item.DESCRIPTION);*/
                            return { label: item.Text, value: item.Value };
                        }))
                    },

                    error: function (err) {
                        alert(err);
                    }

                });

            },

            minLength: 4

        });
        $("#JOBID").autocomplete("widget").addClass('fixed-height');
    };



    $("#PLANTCODE").on("change", function () {
        DDLFamilyByPlant();
    });
    $("#PLANTCODE").on("change", function () {
        BindStage();
    });
    $("#FAMILYCODE").on("change", function () {
        BindStage();
    });
    //Function to bind Plant
    function DDLPlant() {
        //$("#divLoader").show();
        $.ajax({
            url: '@Url.Action("BindPlant", "RollDown")',
            type: "GET",
            contentType: "application/json;charset=UTF-8",
            success: function (result) {
                $.each(result, function (i) {
                    var optionhtml = '<option value="' + result[i].Value + '">' + result[i].Text + '</option>';
                    $("#PLANTCODE").append(optionhtml);
                });


                DDLFamilyByPlant();
        },
            error: function (errormessage) {
            //alert(errormessage.responseText);
            }
        });
    };
    function BindStage() {
        var plantselectedValue = $("#PLANTCODE").val();
        var familyselectedValue = $("#FAMILYCODE").val();
        //$("#divLoader").show();
        $.ajax({
            url: '@Url.Action("BindStages", "RollDown")',
            type: "POST",
            contentType: "application/json;charset=UTF-8",
            data: JSON.stringify({ Plant: plantselectedValue, family: familyselectedValue }),
            success: function (result) {
                $.each(result, function (i) {
                    var optionhtml = '<option value="' + result[i].Value + '">' + result[i].Text + '</option>';
                    $("#STAGE_Code").append(optionhtml);
                });

        },
            error: function (errormessage) {
            //alert(errormessage.responseText);
            }
        });
    };
    //Function to bind Family by Plant
    function DDLFamilyByPlant() {
        //$("#divLoader").show();
        var selectedValue = $("#PLANTCODE").val();
        $.ajax({
            url: '@Url.Action("BindFamily", "RollDown")',
            type: "POST",
            contentType: "application/json;charset=UTF-8",
            data: JSON.stringify({ Plant: selectedValue }),
            success: function (result) {
                $("#FAMILYCODE").html(null);
                $.each(result, function (i) {
                    var optionhtml = '<option value="' + result[i].Value + '">' + result[i].Text + '</option>';
                    $("#FAMILYCODE").append(optionhtml);
                });
                //BindGrid();
        },
            error: function (errormessage) {
            //alert(errormessage.responseText);
            }
        });
    };

    $('#JOBID').on("change", function () {
        gleSearch_EditValueChanged();
        //NewTractorCode();
    });


    function gleSearch_EditValueChanged() {

        if ($('#JOBID').val() != "") {
            var itemCode = $('#JOBID').val();
            const myArray = itemCode.split("#");
            var Data = {
                PLANTCODE: $('#PLANTCODE').val(),
                FAMILYCODE: $('#FAMILYCODE').val(),
                JOBID: itemCode
            };
        } else {
            return;
        }

        $.ajax({
            url: '@Url.Action("BindData", "RollDown")',
            data: JSON.stringify({ obj: Data }),
            type: "POST",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (data) {


                if (data.Msg != "") {
                    $('#alert').append('<div class="alert alert-danger role = "alert"><strong>' + data.Msg + '</strong><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');

                    setTimeout(function () {
                        $.each($('.alert'), function () {
                            closeAlert(this);
                        });
                    }, 5000);

                }
                else {

                    if (data.Result.Transmission != "" && data.Result.Transmission != null) {
                        $("#Transmission").val(data.Result.Transmission);
                    }
                    if (data.Result.RearAxel != "" && data.Result.RearAxel != null) {
                        $("#RearAxel").val(data.Result.RearAxel);
                    }
                    if (data.Result.Engine != "" && data.Result.Engine != null) {
                        $("#Engine").val(data.Result.Engine);
                    }
                    if (data.Result.Backend != "" && data.Result.Backend != null) {
                        $("#Backend").val(data.Result.Backend);
                    }
                    if (data.Result.Hydraulic != "" && data.Result.Hydraulic != null) {
                        $("#Hydraulic").val(data.Result.Hydraulic);
                    }
                    if (data.Result.RearSrnn1 != "" && data.Result.RearSrnn1 != null) {
                        $("#RearSrnn1").val(data.Result.RearSrnn1);
                    }
                    if (data.Result.RearSrnn2 != "" && data.Result.RearSrnn2 != null) {
                        $("#RearSrnn2").val(data.Result.RearSrnn2);
                    }
                    if (data.Result.FrontSrnn1 != "" && data.Result.FrontSrnn1 != null) {
                        $("#FrontSrnn1").val(data.Result.FrontSrnn1);
                    }
                    if (data.Result.FrontSrnn2 != "" && data.Result.FrontSrnn2 != null) {
                        $("#FrontSrnn2").val(data.Result.FrontSrnn2);
                    }
                    if (data.Result.Battery != "" && data.Result.Battery != null) {
                        $("#Battery").val(data.Result.Battery);
                    }
                    if (data.Result.HydrualicPump != "" && data.Result.HydrualicPump != null) {
                        $("#HydrualicPump").val(data.Result.HydrualicPump);
                    }
                    if (data.Result.SteeringMotor != "" && data.Result.SteeringMotor != null) {
                        $("#SteeringMotor").val(data.Result.SteeringMotor);
                    }
                    if (data.Result.SteeringAssem != "" && data.Result.SteeringAssem != null) {
                        $("#SteeringAssem").val(data.Result.SteeringAssem);
                    }
                    if (data.Result.SteeringCylinder != "" && data.Result.SteeringCylinder != null) {
                        $("#SteeringCylinder").val(data.Result.SteeringCylinder);
                    }
                    if (data.Result.Radiator != "" && data.Result.Radiator != null) {
                        $("#Radiator").val(data.Result.Radiator);
                    }
                    if (data.Result.Cluster != "" && data.Result.Cluster != null) {
                        $("#Cluster").val(data.Result.Cluster);
                    }
                    if (data.Result.Alternator != "" && data.Result.Alternator != null) {
                        $("#Alternator").val(data.Result.Alternator);
                    }
                    if (data.Result.Motor != "" && data.Result.Motor != null) {
                        $("#Motor").val(data.Result.Motor);
                    }
                    if (data.Result.RearRIM1 != "" && data.Result.RearRIM1 != null) {
                        $("#RearRIM1").val(data.Result.RearRIM1);
                    }
                    if (data.Result.RearRIM2 != "" && data.Result.RearRIM2 != null) {
