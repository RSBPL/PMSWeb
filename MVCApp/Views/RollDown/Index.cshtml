        var chkRepjob = false;
        if ($('input[name="radio"]:checked').val() =="ReplaceDiv") {
            let text = "Are You Sure To Replace JOBID !!";
            if (confirm(text) == true) {
                chkRepjob = true;
            } else {
                chkRepjob = false;
            }
        }
        var Data = {
            JOBID: jobid,
            RepJOBID: $('#RepJOBID').val(),
            CHANGEJOB_NO: $('#CHANGEJOB_NO').val(),
            PLANTCODE: $('#PLANTCODE').val(),
            FAMILYCODE: $('#FAMILYCODE').val(),
            Engine_srlno:$("#Engine_srlno").val(),
            Transmission_srlno:$("#Transmission_srlno").val(),
            RearAxel_srlno:$("#RearAxel_srlno").val(),
            Backend_srlno:$("#Backend_srlno").val(),
            STAGE_Code:$("#STAGE_Code").val(),
            Hydraulic_srlno:$("#Hydraulic_srlno").val(),
            RearTyre1_srlno1:$("#RearTyre1_srlno1").val(),
            RearTyre2_srlno2:$("#RearSrnn2_srlno2").val(),
            FrontSrnn1_srlno1:$("#FrontSrnn1_srlno1").val(),
            FrontSrnn2_srlno2:$("#FrontSrnn2_srlno2").val(),
            Battery_srlno:$("#Battery_srlno").val(),
            BatMake:$("#BatMake").val(),
            HydrualicPump_srlno:$("#HydrualicPump_srlno").val(),
            SteeringMotor_srlno:$("#SteeringMotor_srlno").val(),
            SteeringAssem_srlno:$("#SteeringAssem_srlno").val(),
            SteeringCylinder_srlno:$("#SteeringCylinder_srlno").val(),
            Radiator_srlno:$("#Radiator_srlno").val(),
            Cluster_srlno:$("#Cluster_srlno").val(),
            Alternator_srlno:$("#Alternator_srlno").val(),
            Motor_srlno:$("#Motor_srlno").val(),
            RearRIM1_srlno:$("#RearRIM1_srlno").val(),
            RearRIM2_srlno:$("#RearRIM2_srlno").val(),
            FrontRIM1_srlno:$("#FrontRIM1_srlno").val(),
            FrontRIM2_srlno:$("#FrontRIM2_srlno").val(),
            RearTyre1_srlno1:$("#RearTyre1_srlno1").val(),
            RearTyre2_srlno2:$("#RearTyre2_srlno2").val(),
            FrontTyre1_srlno1:$("#FrontTyre1_srlno1").val(),
            FrontTyre2_srlno2:$("#FrontTyre2_srlno2").val(),
            Srno:$("#Srno").val(),
            IMEI:$("#IMEI").val(),
            isSrNoRequire:false,
            isReplaceJob: chkRepjob,
            MOBILE:$("#MOBILE").val()

        };
        if ($('input[name="radio"]:checked').val() == "ChangeJobDiv") {
            if (Data.CHANGEJOB_NO == "" || Data.CHANGEJOB_NO == null) {
                $('#alert').append('<div class="alert alert-danger role = "alert"><strong>Please Enter Job to continue..</strong><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');
