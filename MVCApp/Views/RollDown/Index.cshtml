                setTimeout(function () {
                    $.each($('.alert'), function () {
                        closeAlert(this);
                    });
                }, 5000);
                $("#CHANGEJOB_NO").focus();
                $("#divLoader").hide();
                return;
            }
            let text = "Are you sure to change the job number of tractor " + Data.TractorSrlno + " Job no " + Data.JOBID + " To CHANGE JOB " + Data.CHANGEJOB_NO + "";
            if (confirm(text) == true) {
                Data.CHANGEJOB_NO = $('#CHANGEJOB_NO').val();
