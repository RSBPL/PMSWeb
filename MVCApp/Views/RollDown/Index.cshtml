    function printData() {

        if ($('#JOBID').val() != "") {
            var jOBID = $('#JOBID').val();
            const myArray = jOBID.split("#");
            var stagecode = $("#STAGE_Code").val();
            if (stagecode == undefined && stagecode == "") {
                $('#alert').append('<div class="alert alert-danger role = "alert"><strong>Please Select Stage</strong><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');

                return;
            }
            //const myArray = itemCode.split("#");
            var Data = {
                PLANTCODE: $('#PLANTCODE').val(),
                FAMILYCODE: $('#FAMILYCODE').val(),
                STAGE_Code: stagecode,
                isBypass: bypass,
                Quantity: $('#Quantity').val(),
                IPAddress: $("#IPAddress").val(),
                Battery_srlno: $("#Battery_srlno").val(),
                RearTyre1_srlno1: $("#RearTyre1_srlno1").val(),
                RearTyre2_srlno2: $("#RearTyre2_srlno2").val(),
                FrontTyre1_srlno1: $("#FrontTyre1_srlno1").val(),
                FrontTyre2_srlno2: $("#FrontTyre2_srlno2").val(),
                Engine_srlno: $("#Engine_srlno").val(),
                Transmission_srlno: $("#Transmission_srlno").val(),
                RearAxel_srlno: $("#RearAxel_srlno").val(),
                Backend_srlno: $("#Backend_srlno").val(),
                Hydraulic_srlno: $("#Hydraulic_srlno").val(),
                HydrualicPump_srlno: $("#HydrualicPump_srlno").val(),
                SteeringMotor_srlno: $("#SteeringMotor_srlno").val(),
                SteeringAssem_srlno: $("#SteeringAssem_srlno").val(),
                SteeringCylinder_srlno: $("#SteeringCylinder_srlno").val(),
                Radiator_srlno: $("#Radiator_srlno").val(),
                Cluster_srlno: $("#Cluster_srlno").val(),
                Alternator_srlno: $("#Alternator_srlno").val(),
                Motor_srlno: $("#Motor_srlno").val(),
                RearRIM1_srlno: $("#RearRIM1_srlno").val(),
                RearRIM2_srlno: $("#RearRIM2_srlno").val(),
                FrontRIM1_srlno: $("#FrontRIM1_srlno").val(),
                FrontRIM2_srlno: $("#FrontRIM2_srlno").val(),
                Srno: $("#Srno").val(),
                IMEI: $("#IMEI").val(),
                MOBILE: $("#MOBILE").val(),
                JOBID: myArray[3]
            };
        } else {
            return;
        }
        $.ajax({
            url: '@Url.Action("PRINTDATA", "RollDown")',
            data: JSON.stringify({ down: Data }),
            type: "POST",
            contentType: "application/json;charset=utf-8",
            dataType: "json",
            success: function (data) {

                if (data.Msg != "successfully Printed") {
                    $('#alert').append('<div class="alert alert-danger role = "alert"><strong>' + data.Msg + '</strong><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');
                    setTimeout(function () {
                        $.each($('.alert'), function () {
                            closeAlert(this);
                        });
                    }, 5000);
                    //setTimeout(function () { $("#alert").hide(); }, 5000)
                }

                else {
                    $('#alert').append('<div class="alert alert-success role = "alert"><strong>' + data.Msg + '</strong><button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>');
                    //setTimeout(function () { $("#alert").hide(); }, 5000);
                    setTimeout(function () { $(".alert").alert('close'); }, 5000); setTimeout(function () {
                        $.each($('.alert'), function () {
                            closeAlert(this);
                        });
                    }, 5000);
                    /* $('#VendorName').val("");*/
                    /*  $('#VendorName').val("");*/
                    $('#ItemCode').val("");
                    $('#DeviationQty').val("");
                    $('#AprovedBy').val("");
                    BindGrid();
                }
            },
            error: function (errormessage) {

            }
        });

    };
    function closeAlert(alert) {

        $(alert).hide();
    };
