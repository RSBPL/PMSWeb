                        //setTimeout(function () { $(".alert").alert('close'); }, 5000);
                        setTimeout(function () {
                            $.each($('.alert'), function () {
                                closeAlert(this);
                            });
                        }, 5000);

                    }

                }
            },
            error: function (errormessage) {

            }
        });
    }
