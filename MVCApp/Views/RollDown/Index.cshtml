    function Update() {
        $("#divLoader").show();
        var itemCode = $('#JOBID').val();
        const myArray = itemCode.split("#");
        var tractorSrlno = myArray[2];
        var jobid = myArray[3];
        if (myArray.length < 2) {
            jobid = myArray[0];
        }
