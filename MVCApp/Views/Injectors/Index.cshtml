                    <span class="col-md-1 pull-right m-top5">
                        <img ID="img" style="width: 100%; height: 60px;" />
                    </span>
                </div>
            </div>

            @*<div class="row m-top10" id="tdMsg" runat="server" visible="true">
                    <div class="col-md-6 padding-right-0">
                        <span class="col-md-12 btn-warning btn-lg" style="background-color: #f0ad4e; font-family: 'Arial Rounded MT'; font-size: 20px;">
