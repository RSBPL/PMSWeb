                            <div ID="lblInfodb" Text="InfoDB" EncodeHtml="false" runat="server"></div>
                        </span>
                    </div>
                </div>*@
            <div class="col-md-12 m-top10" id="tdError">
                <div class="col-md-12 btn-danger btn-lg">
                    <span style="width: 100%; font-family: 'Arial Rounded MT'; font-size: 20px;">
                        <i class="fa fa-times" aria-hidden="true"></i>
                        <label ID="lblError"></label>&nbsp;&nbsp;&nbsp;&nbsp;
                        <label ID="lblErrordb"></label>

                    </span>
                </div>
            </div>
            <div class="col-md-12 m-top10">
                <div class="col-md-12">
                    <div class="col-md-12">
                        <div class="col-md-6 padding-left-0 padding-right-half">
                            <span class="">
                                <label ID="lblDcode" Width="100%" class="text-center enginDispaly"></label>
                            </span>
                        </div>
                        <div class="col-md-6 padding-left-0 padding-right-half">
                            <span class="">
                                <label ID="lblSrlno" Width="100%" class="text-center enginDispaly"></label>
                            </span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-12 m-top10">
                <div class="col-md-12">
                    <div class="col-md-12">
                        <div class="col-md-12">
                            <div class="col-md-1"></div>
                            <div class="col-md-10">
                                <div class="col-md-6 padding-left-0 padding-right-half">
                                    <span class="col-md-12 btn-lg setbgH setbgINJ text-center">
                                        <label ID="lblInj1" class="setHtext"></label>
                                        <br /><font size=3>(TIMING SIDE)</font>
