        </div>
        <div class="col-md-10">
            <strong class="head-header" style="margin-top: -54px;">AFTER PAINT LCD2 DISPLAY</strong>
        </div>
    </div>
</div>

<div style="width: 100%">
    <div style="color: #fff; background-color: #065499; border-color: #428bca; height: 45px; padding: 0; text-align: center; vertical-align: central;">
        <table style="width: 100%">
            <tr style="font-size: 28px; FONT-WEIGHT: 600;" class="tr2">
                <td align="center" style="WIDTH: 20%;">
                    <label class="text-right control-label col-form-label" id="lblDate">&nbsp;&nbsp;&nbsp;@ViewBag.lblDate</label>
                </td>
                <td align="center" style="WIDTH: 20%;">
                    <label class="text-right control-label col-form-label" id="lblTime">&nbsp;&nbsp;&nbsp;@ViewBag.Time</label>
                </td>
                <td align="center" style="WIDTH: 20%;">
                    <label class="text-right control-label col-form-label" id="lblShift">&nbsp;&nbsp;&nbsp;@ViewBag.Shift</label>
                </td>
                <td align="center" style="WIDTH: 20%;">
                    <label class="text-right control-label col-form-label" id="lblHookDown">&nbsp;&nbsp;&nbsp;@ViewBag.lblHookDown</label>
                </td>
                <td align="center" style="WIDTH: 20%;">
                    <label class="text-right control-label col-form-label" id="lblDayTotal">&nbsp;&nbsp;&nbsp;@ViewBag.lblDayTotal</label>
                </td>
                <td style="visibility:@ViewBag.imgstatusHS">
                    <img src="@ViewBag.imgstatus">
                </td>
            </tr>
        </table>
    </div>
</div>
<div class="page-content">
    @if (ViewBag.lblInfoTF == true)
    {
        <div class="alert alert-info" role="alert">
            <strong>@ViewBag.lblInfo</strong>
            <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
