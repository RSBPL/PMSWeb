                    <!--<td style="padding: 5px;">@d.QTY</td>
                    <td style="padding: 5px;">@d.HOOK</td>
                    <td style="padding: 5px;">@d.BATTERY</td>
                    <td style="padding: 5px;">@d.HEAD_LAMP</td>
