                    <td style="padding: 5px;font-size: 15px;font-weight: 700;text-align: center;">@d.FENDER_LAMP</td>
                    <td style="padding: 5px;font-size: 15px;font-weight: 700;text-align: center;">@d.PLAN</td>
                    <td style="padding: 5px;font-size: 15px;font-weight: 700;text-align: center;">@d.DONE</td>
                    <td style="padding: 5px;font-size: 15px;font-weight: 700;text-align: center;">@d.PENDING</td>



